
<%= render 'form', test: @test %>
<div class="col-md-4 col-md-offset-4">
  <div class="well" id="uploads-table">

      <br>
    
    <table class="table table-striped table-dark">
      <tbody>
        <tr>
          <th>List of Uploads</th>
          <th colspan="3"></th>
        </tr>
        <% @tests.each do |test| %>
          <tr>
            <td><%= test.name %></td> 
            <td><%= link_to 'Page 2', test %></td>
            <td><%= link_to 'Destroy', test, method: :delete, data: { confirm: 'Are you sure?'} %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>




<%#= link_to 'New Test', new_test_path %>
